#!/bin/bash

function g_next() {
	CURR_COMMIT=$(git rev-parse HEAD)
	git checkout $(git log --branches -1 --pretty=format:"%H")
	NEXT_COMMIT=$(git rev-list "$CURR_COMMIT"..HEAD | tail -1)
	git checkout "$NEXT_COMMIT"
}

function g_prev() {
	git checkout HEAD^1
}

function awsl() {
	acccount=$1
	shift
	aws-vault login $account -d 8h "$@"
}

function awse() {
	acccount=$1
	shift
	aws-vault exec $acccount -d 8h "$@"
}
